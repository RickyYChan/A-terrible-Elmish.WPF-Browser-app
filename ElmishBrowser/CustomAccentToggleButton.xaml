<UserControl x:Class="ElmishBrowser.CustomAccentToggleButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ElmishBrowser"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             x:Name="Self"
             >
    <Button Style="{DynamicResource ButtonAccentRevealStyle}"
            Content="{Binding ElementContent, ElementName=Self}"
            Background="{Binding ElementBackground, ElementName=Self}"
            BorderThickness="{Binding ElementBorderThickness, ElementName=Self}"
            BorderBrush="{Binding ElementBorderBrush, ElementName=Self}"
            Padding="{Binding ElementPadding, ElementName=Self}"
            Click="Self_Click"
            />
</UserControl>
